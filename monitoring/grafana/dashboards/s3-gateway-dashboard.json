{
  "dashboard": {
    "id": null,
    "title": "S3 Gateway Metrics",
    "tags": ["s3", "versitygw", "gateway"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 0,
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(versitygw_success_requests_total[5m]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          },
          {
            "expr": "sum(rate(versitygw_failed_requests_total[5m]))",
            "legendFormat": "Error Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Data Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(versitygw_bytes_read_total[5m]))",
            "legendFormat": "Read Throughput",
            "refId": "A"
          },
          {
            "expr": "sum(rate(versitygw_bytes_written_total[5m]))",
            "legendFormat": "Write Throughput",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes/sec",
            "unit": "binBps"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Object Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(versitygw_objects_created_total[5m]))",
            "legendFormat": "Objects Created",
            "refId": "A"
          },
          {
            "expr": "sum(rate(versitygw_objects_removed_total[5m]))",
            "legendFormat": "Objects Removed",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Objects/sec",
            "unit": "ops"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 4,
        "title": "Error Rate by Action",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(versitygw_failed_requests_total[5m])) by (action)",
            "legendFormat": "{{ action }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "unit": "reqps"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 5,
        "title": "Request Distribution by Action",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(versitygw_success_requests_total[5m])) by (action)",
            "legendFormat": "{{ action }}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(versitygw_request_duration_bucket[5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(versitygw_request_duration_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(versitygw_request_duration_bucket[5m])) by (le))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time",
            "unit": "s"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 12
        }
      }
    ]
  }
}